        {
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "P44368ADAD746BC27"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "\nWITH daily_users AS (\n    SELECT \n        DATE(joined_at) AS join_date,\n        COUNT(*) AS users_joined\n    FROM \n        sensei_users\n    WHERE \n        joined_at >= NOW() - INTERVAL '30 days'\n    GROUP BY \n        DATE(joined_at)\n), all_days AS (\n    SELECT \n        generate_series(\n            CURRENT_DATE - INTERVAL '29 days', \n            CURRENT_DATE, \n            '1 day'\n        )::DATE AS join_date\n)\nSELECT \n    ad.join_date,\n    COALESCE(SUM(du.users_joined) OVER (ORDER BY ad.join_date), 0) AS total_users\nFROM \n    all_days ad\nLEFT JOIN \n    daily_users du\nON \n    ad.join_date = du.join_date\nORDER BY \n    ad.join_date;",
          "refId": "A",
